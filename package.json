{
  "name": "firstory-podcast-automation",
  "version": "1.0.0",
  "description": "Automated podcast upload system for Firstory using Playwright",
  "main": "index.js",
  "scripts": {
    "start": "node run-automation.js once",
    "upload": "node run-automation.js once",
    "test": "node run-automation.js test",
    "scheduled": "node run-automation.js scheduled",
    "cleanup": "node run-automation.js cleanup",
