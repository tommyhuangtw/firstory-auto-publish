    "download": "node download-latest-files.js",
    "download-advanced": "node advanced-drive-downloader.js",
    "download-api": "node download-with-api.js",
    "setup-google-api": "node setup-google-api.js",
    "setup-guide": "node setup-drive-links.js",
    "test-google-api": "node test-google-api.js",
    "simple-oauth": "node simple-oauth.js",
    "oauth-fix": "node oauth-fix.js",
    "oauth-simple": "node oauth-simple.js",
    "webhook": "node webhook-server.js",
